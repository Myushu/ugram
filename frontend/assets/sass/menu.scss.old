body {
    background-color: #fafafa;
    font-family: Helvetica;
}

.navbar-default {
    background-color: #fff;
    margin: 0;
    height: 80px;
    .container-fluid {
        width: 50%;
        .navbar-header {
            .navbar-brand {
                img {
                    width: 150px;
                }
            }
        }
        .navbar-right {
            font-size: 25px;
            i {
                line-height: 50px;
            }
        }
    }

}