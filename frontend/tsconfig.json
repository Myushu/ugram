{
  "exclude": [
    "node_modules"
  ],
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": false,
    "noImplicitAny": false,
    "baseUrl": ".",
    "outDir": "./dist/out-tsc"
  },
  "types": [
    "core-js"
  ]
}
